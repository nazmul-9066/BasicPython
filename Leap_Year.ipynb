   "source": [
    "# User enters the year\n",
    "year = int(input(\"Enter Year: \"))\n",
    "\n",
    "# Leap Year Check\n",
    "if year % 4 == 0 and year % 100 != 0 and year%400 == 0:\n",
    "    print(year, \"is a Leap Year\")\n",
    "else\n",
    "    print(year, \"is not a Leap Year\")"
